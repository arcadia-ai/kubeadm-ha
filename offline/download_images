#!/bin/bash

docker pull registry.cn-shanghai.aliyuncs.com/kubeadm-ha/setzero_ansible:2.8.5-nginx-1.17.6-alpine
docker pull registry.cn-shanghai.aliyuncs.com/kubeadm-ha/openresty_openresty:1.17.8.2-alpine
docker pull registry.cn-shanghai.aliyuncs.com/kubeadm-ha/kubernetesui_dashboard:v2.0.4 
docker pull registry.cn-shanghai.aliyuncs.com/kubeadm-ha/kubernetesui_metrics-scraper:v1.0.4
docker pull registry.aliyuncs.com/k8sxio/kube-proxy:v1.18.6 
docker pull registry.aliyuncs.com/k8sxio/kube-apiserver:v1.18.6 
docker pull registry.aliyuncs.com/k8sxio/kube-scheduler:v1.18.6 
docker pull registry.aliyuncs.com/k8sxio/kube-controller-manager:v1.18.6 
docker pull registry.aliyuncs.com/k8sxio/coredns:1.6.7 
docker pull registry.aliyuncs.com/k8sxio/etcd:3.4.3-0 
docker pull registry.cn-shanghai.aliyuncs.com/kubeadm-ha/metrics-server_metrics-server:v0.4.0
docker pull registry.aliyuncs.com/k8sxio/pause:3.2 
docker pull registry.cn-shanghai.aliyuncs.com/kubeadm-ha/calico_typha:v3.16.5
docker pull registry.cn-shanghai.aliyuncs.com/kubeadm-ha/calico_cni:v3.16.5
docker pull registry.cn-shanghai.aliyuncs.com/kubeadm-ha/calico_node:v3.16.5
docker pull registry.cn-shanghai.aliyuncs.com/kubeadm-ha/calico_kube-controllers:v3.16.5
docker pull registry.cn-shanghai.aliyuncs.com/kubeadm-ha/calico_pod2daemon-flexvol:v3.16.5
docker pull registry.cn-shanghai.aliyuncs.com/kubeadm-ha/calico_ctl:v3.16.5
docker pull registry.cn-shanghai.aliyuncs.com/kubeadm-ha/ingress-nginx_controller:v0.41.0 

docker save \
    registry.cn-shanghai.aliyuncs.com/kubeadm-ha/setzero_ansible:2.8.5-nginx-1.17.6-alpine \
    registry.cn-shanghai.aliyuncs.com/kubeadm-ha/openresty_openresty:1.17.8.2-alpine \
    registry.cn-shanghai.aliyuncs.com/kubeadm-ha/kubernetesui_dashboard:v2.0.4 \
    registry.cn-shanghai.aliyuncs.com/kubeadm-ha/kubernetesui_metrics-scraper:v1.0.4 \
    registry.aliyuncs.com/k8sxio/kube-proxy:v1.18.6 \
    registry.aliyuncs.com/k8sxio/kube-apiserver:v1.18.6 \
    registry.aliyuncs.com/k8sxio/kube-scheduler:v1.18.6 \
    registry.aliyuncs.com/k8sxio/kube-controller-manager:v1.18.6 \
    registry.aliyuncs.com/k8sxio/coredns:1.6.7 \
    registry.aliyuncs.com/k8sxio/etcd:3.4.3-0 \
    registry.cn-shanghai.aliyuncs.com/kubeadm-ha/metrics-server_metrics-server:v0.4.0 \
    registry.aliyuncs.com/k8sxio/pause:3.2 \
    registry.cn-shanghai.aliyuncs.com/kubeadm-ha/ingress-nginx_controller:v0.41.0 \
    registry.cn-shanghai.aliyuncs.com/kubeadm-ha/calico_typha:v3.16.5 \
    registry.cn-shanghai.aliyuncs.com/kubeadm-ha/calico_cni:v3.16.5 \
    registry.cn-shanghai.aliyuncs.com/kubeadm-ha/calico_node:v3.16.5 \
    registry.cn-shanghai.aliyuncs.com/kubeadm-ha/calico_kube-controllers:v3.16.5 \
    registry.cn-shanghai.aliyuncs.com/kubeadm-ha/calico_pod2daemon-flexvol:v3.16.5 \
    registry.cn-shanghai.aliyuncs.com/kubeadm-ha/calico_ctl:v3.16.5 | gzip -1 > /kubernetes/kubernetes-1.18.6.tar